[alias]
	co = checkout
	ci = commit
	branches = branch -a
	tags = tag -l
	st = status
	graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	diffv = diff --word-diff
	diff-name = diff --name-only
[core]
	editor = vim
[color]
	ui = auto
